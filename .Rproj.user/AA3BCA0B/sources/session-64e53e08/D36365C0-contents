#   geom_errorbar(aes(x = log2FoldChange, y = pathway, color = Contrast, group = Contrast, xmin = log2FoldChange - lfcSE, xmax = log2FoldChange + lfcSE), position = position_jitter(seed = 1234), width = 0.2)+
#   geom_vline(xintercept = 0)
